<h1>New Trait Type</h1>


<%= form_tag(trait_types_url, method: 'post') do %>


  <div>
    <%= label_tag :name %><br />
    <%= text_field_tag :name %>
  </div>

    <div>
    <%= label_tag :image_url %><br />
    <%= text_field_tag :image_url %>
  </div>

    <div>
    <%= label_tag :description %><br />
    <%= text_field_tag :description %>
  </div>

  
  <div>
    <%= submit_tag "Create Trait Type" %>
  </div>
<% end %>
