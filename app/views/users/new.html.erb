<h1>New User</h1>


<%= form_tag(users_url, method: 'post') do %>


  <div>
    <%= label_tag :first_name %><br />
    <%= text_field_tag :first_name %>
  </div>

    <div>
    <%= label_tag :last_name %><br />
    <%= text_field_tag :last_name %>
  </div>

    <div>
    <%= label_tag :full_name %><br />
    <%= text_field_tag :full_name %>
  </div>

    <div>
    <%= label_tag :email %><br />
    <%= text_field_tag :email %>
  </div>

    <div>
    <%= label_tag :uid %><br />
    <%= number_field_tag :uid %>
  </div>

    <div>
    <%= label_tag :access_token %><br />
    <%= text_field_tag :access_token %>
  </div>

    <div>
    <%= label_tag :provider %><br />
    <%= text_field_tag :provider %>
  </div>

    <div>
    <%= label_tag :avatar %><br />
    <%= text_field_tag :avatar %>
  </div>

  
  <div>
    <%= submit_tag "Create User" %>
  </div>
<% end %>
