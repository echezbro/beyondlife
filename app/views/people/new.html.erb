<h1>New Person</h1>


<%= form_tag(people_url, method: 'post') do %>


  <div>
    <%= label_tag :dob %><br />
    <%= datetime_select_tag :dob %>
  </div>

    <div>
    <%= label_tag :dod %><br />
    <%= datetime_select_tag :dod %>
  </div>

    <div>
    <%= label_tag :full_name %><br />
    <%= text_field_tag :full_name %>
  </div>

    <div>
    <%= label_tag :quote %><br />
    <%= text_field_tag :quote %>
  </div>

    <div>
    <%= label_tag :eulogy %><br />
    <%= text_field_tag :eulogy %>
  </div>

    <div>
    <%= label_tag :charity %><br />
    <%= text_field_tag :charity %>
  </div>

    <div>
    <%= label_tag :photo_url %><br />
    <%= text_field_tag :photo_url %>
  </div>

  
  <div>
    <%= submit_tag "Create Person" %>
  </div>
<% end %>
